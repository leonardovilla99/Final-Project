import java.util.Random;

public class AccountNumber {
    public static String accNumRandomizer() {
        Random rand=new Random();
        int[] numGenerator=new int[6];
        String accNum="";
        for(int i=0;i<numGenerator.length;i++) {
            numGenerator[i]=rand.nextInt(9);
            accNum+=numGenerator[i];
        }
        return accNum;
    }
}